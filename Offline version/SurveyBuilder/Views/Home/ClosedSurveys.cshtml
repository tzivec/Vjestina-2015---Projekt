<fieldset>
    <legend>Closed Surveys</legend>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Name</th>
                <th>Description</th>
                <th>Number of Questions</th>
                <th>Statistics</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="survey in surveys">
                <td ng-bind="survey.Name"></td>
                <td ng-bind="survey.Description"></td>
                <td ng-bind="survey.Questions.length"></td>
                <td class="btn"><a ng-href="/#/statistics/{{survey.SurveyId}}">See Statistics</a></td>
            </tr>
        </tbody>
    </table>
</fieldset>
